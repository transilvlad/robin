{
  // Path to MTA logs if one running on local host.
  localLogsDir: "/usr/local/robin/log/",

  // Pattern to match UID out of SMT responses.
  uidPattern: "\\s\\[([a-z0-9\\-_]+)]",

  // Client RSET command before additional envelopes.
  rsetBetweenEnvelopes: false,

  // Auto reload properties file config.
  propertiesAutoReload: {
    enabled: false,
    delaySeconds: 300,
    intervalSeconds: 300
  },

  // Auto reload server config.
  serverAutoReload: {
    enabled: false,
    delaySeconds: 300,
    intervalSeconds: 300
  },

  // Logging config.
  logging: {
    data: false, // Log email DATA sent and received.
    textPartBody: false // Log MIME build case text/* type part contents.
  },

  // HTTP Requests configuration.
  request: {
    connectTimeout: 20,
    writeTimeout: 20,
    readTimeout: 90
  },

  // Humio configuration.
  humio: {
    auth: "YOUR_API_KEY",
    url: "https://humio.example.com/",
    connectTimeout: 20,
    writeTimeout: 20,
    readTimeout: 90
  },

  // Enable debug mode for Robin IMAP client.
  imapClientDebug: "false",

  // Test properties for PropertiesTest.
  "digestmd5.random": "64",
  boolean: true,
  long: 7,
  string: "string",
  sub: {
    string: "substring"
  },
  list: [
    "monkey",
    "weasel",
    "dragon"
  ],
  map: {
    string: "map"
  }
}